proc concat_instruction
  call get_next_param
  set checkvarval next_param
  set checkvartyp "string"
  call checkvar
  if checkvarfail
    return
  end

  # Value
  call get_next_param
  set toRef next_param
  call ref
  
  set checkval_typ "string"
  call checkval
  if checkval_fail
    return
  end
  
  # Add code
  set addCode_code ""
  concat addCode_code checkvar_code
  concat addCode_code " += "
  concat addCode_code ref_code
  concat addCode_code ";\n"
  call addCode
end