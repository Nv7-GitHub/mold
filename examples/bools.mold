# Equality demo
print "Equality"
set a 1
set b 1
set equal false

eq equal a b
print equal

set b 2
eq equal a b
print equal

# If statements, if (1 == 1)
set a 1
set b 1

set cond false
eq cond a b

if cond
  print "Good"
else
  print "Bad"
end

# While statements using while (i < 10) {i++}
set i 1
set target 10

set cond false
less cond i target

while cond
  print i

  add i 1
  set cond false
  less cond i target
end